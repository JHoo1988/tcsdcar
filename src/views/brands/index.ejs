                    <img src="../../static/images/icon_tabbar.png" alt="">
                </div>
                <p class="weui-grid__label">Grid</p>
            </a>
            <a href="javascript:;" class="weui-grid">
                <div class="weui-grid__icon">
                    <img src="../../static/images/icon_tabbar.png" alt="">
                </div>
                <p class="weui-grid__label">Grid</p>
            </a>
        </div>
        <% if(len > 0 ){ %>
        <div class="weui_cells weui_cells_access list-wrapper phone-list">
            <%
            for(var i = 0;i < len;i++){
                var d = data[i];
            %>
            <div class="brands_szm">
                <span><%= d.firstChar %></span>
            </div>
            <%
                var content = d.content;
            for(var j = 0;j < content.length;j++){ %>
            <a class="weui_cell list-item" href="javascript:;" data-name="<%= content[j].name %>"
               data-json='<%= JSON.stringify(content[j]) %>'>
                <div class="weui_cell_hd img-wrapper">
                    <img src="../../../images/brand/<%= content[j].id %>.png" alt="icon" class="img-icon">
                </div>
                <div class="weui_cell_bd weui_cell_primary">
                    <p>
                        <span><%= content[j].name %></span>
                    </p>
                </div>
                <div class="weui_cell_ft"></div>
            </a>
            <% } %>
            <% } %>
        </div>
        <%}else{%>
        <p class="no-data">暂无品牌</p>
        <%}%>
    </main>
</div>

